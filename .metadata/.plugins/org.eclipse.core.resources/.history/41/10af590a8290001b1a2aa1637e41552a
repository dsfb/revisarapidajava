package br.com.enums.controller;

public enum Marcas {
	
	 VOLKSWAGEM("Volkswagen"),
	 HYUNDAI("Hyundai"),
	 FIAT("Fiat"),
	 HONDA("Honda");
	 
	 
	 private String descricao;

	private Marcas(String descricao) {
		this.descricao = descricao;
	}
	 
	 public String getDescricao() {
		 
		 return descricao;
	 }

}
